<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <LinearLayout
            android:orientation="horizontal"
            android:background="@color/color_donate_item"
            android:padding="10.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip"
            android:layout_marginStart="10.0dip"
            android:layout_marginEnd="10.0dip">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0">
                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@id/billtwoimg"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="38.0dip"
                        android:layout_marginBottom="10.0dip"
                        android:src="@mipmap/ic_user_portrait"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="10.0dip"
                        app:civ_border_color="@color/colorPrimary"
                        app:civ_border_width="0.0dip" />
                    <ImageView
                        android:layout_gravity="right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30.0dip"
                        android:src="@mipmap/donatebilltoprighttwo"
                        android:layout_marginEnd="14.0dip" />
                </FrameLayout>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="28.0dip"
                    android:layout_weight="1.0">
                    <TextView
                        android:textSize="12.0sp"
                        android:textColor="@color/white"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@id/billnametwo"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:maxLines="1"
                        android:layout_marginStart="1.0dip" />
                    <ImageView
                        android:id="@id/billcountrytwoimg"
                        android:layout_width="22.0dip"
                        android:layout_height="15.0dip"
                        android:src="@drawable/ic_region_china"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="3.0dip" />
                </androidx.appcompat.widget.LinearLayoutCompat>
                <TextView
                    android:textSize="14.0sp"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@id/billmoneytwo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="0.8">
                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@id/billoneimg"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="2.0dip"
                        android:src="@mipmap/ic_user_portrait"
                        android:layout_centerVertical="true"
                        app:civ_border_color="@color/colorPrimary"
                        app:civ_border_width="0.0dip" />
                    <ImageView
                        android:layout_gravity="right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0.0dip"
                        android:src="@mipmap/donatebilltoprightone"
                        android:layout_marginEnd="43.0dip" />
                </FrameLayout>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="28.0dip">
                    <TextView
                        android:textSize="16.0sp"
                        android:textColor="@color/text_color_pro"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@id/billnameone"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:maxLines="1"
                        android:layout_marginStart="1.0dip" />
                    <ImageView
                        android:id="@id/billcountryoneimg"
                        android:layout_width="22.0dip"
                        android:layout_height="15.0dip"
                        android:src="@drawable/ic_region_china"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="3.0dip" />
                </androidx.appcompat.widget.LinearLayoutCompat>
                <TextView
                    android:textSize="20.0sp"
                    android:textColor="@color/text_color_pro"
                    android:gravity="center"
                    android:id="@id/billmoneyone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0">
                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@id/billthreeimg"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="38.0dip"
                        android:layout_marginBottom="10.0dip"
                        android:src="@mipmap/ic_user_portrait"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="10.0dip"
                        app:civ_border_color="@color/colorPrimary"
                        app:civ_border_width="0.0dip" />
                    <ImageView
                        android:layout_gravity="right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30.0dip"
                        android:src="@mipmap/donatebilltoprightthree"
                        android:layout_marginEnd="14.0dip" />
                </FrameLayout>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="28.0dip"
                    android:layout_weight="1.0">
                    <TextView
                        android:textSize="12.0sp"
                        android:textColor="@color/white"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@id/billnamethree"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:maxLines="1"
                        android:layout_marginStart="1.0dip" />
                    <ImageView
                        android:id="@id/billcountrythreeimg"
                        android:layout_width="22.0dip"
                        android:layout_height="15.0dip"
                        android:src="@drawable/ic_region_china"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="3.0dip" />
                </androidx.appcompat.widget.LinearLayoutCompat>
                <TextView
                    android:textSize="12.0sp"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@id/billmoneythree"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@id/donate_show_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip"
            android:text="@string/no_ranking"
            android:layout_marginStart="10.0dip" />
        <include
            android:layout_gravity="center"
            android:id="@id/donate_mine_layout"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:layout_margin="10.0dip"
            layout="@layout/layout_donatebill_item" />
        <androidx.recyclerview.widget.RecyclerView
            android:id="@id/addons_rv"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="4.0dip"
            android:layout_marginStart="10.0dip"
            android:layout_marginEnd="10.0dip" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>