<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:background="@color/colorPrimaryDark"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@id/mine_user_info_layout"
        android:background="@color/colorPrimaryDark"
        android:fitsSystemWindows="true"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            app:layout_scrollFlags="enterAlways|scroll">
            <include
                android:id="@id/layout_titlebar_simple"
                layout="@layout/layout_titlebar_simple" />
            <include
                android:id="@id/layout_user_info"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="68.0dip"
                android:layout_marginRight="68.0dip"
                layout="@layout/layout_user_info" />
            <LinearLayout
                android:orientation="horizontal"
                android:id="@id/logged_debug_line"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/activity_horizontal_spacing"
                android:layout_marginRight="168.0dip"
                android:layout_alignParentRight="true">
                <TextView
                    android:textSize="12.0sp"
                    android:id="@id/txt_user"
                    android:background="@drawable/style_btn"
                    android:padding="4.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/activity_horizontal_spacing"
                    android:text="用户数据"
                    android:textAlignment="center" />
                <TextView
                    android:textSize="12.0dip"
                    android:id="@id/btn_clear"
                    android:background="@drawable/style_btn"
                    android:padding="4.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="clear" />
            </LinearLayout>
        </RelativeLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.core.widget.NestedScrollView
        android:id="@id/logged_main_content"
        android:paddingLeft="@dimen/activity_horizontal_spacing"
        android:paddingRight="@dimen/activity_horizontal_spacing"
        android:scrollbars="none"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fillViewport="true"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6.0dip">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1.0">
                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@id/layout_show_pro"
                        android:background="@mipmap/background_vip"
                        android:layout_width="fill_parent"
                        android:layout_height="60.0dip">
                        <androidx.appcompat.widget.AppCompatTextView
                            android:textSize="@dimen/text_size_title"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray_deep"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_spacing"
                            android:text="@string/my_pro_title" />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:textSize="@dimen/text_size_tiny"
                            android:textStyle="bold"
                            android:textColor="@color/text_color_gray_deep"
                            android:gravity="left"
                            android:id="@id/buy_pro_price_ori_tv"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_spacing"
                            android:text="@string/product_pro_description" />
                    </LinearLayout>
                    <com.google.android.material.tabs.TabLayout
                        android:id="@id/product_tabs"
                        android:background="@color/color_product_pro"
                        android:layout_width="fill_parent"
                        android:layout_height="40.0dip"
                        app:tabIndicatorColor="@color/white"
                        app:tabIndicatorHeight="1.0dip"
                        app:tabMode="fixed"
                        app:tabSelectedTextColor="@color/white"
                        app:tabTextAppearance="@style/TabLayoutTextStyle"
                        app:tabTextColor="@color/text_color_gray_deep" />
                </FrameLayout>
                <androidx.viewpager.widget.ViewPager
                    android:id="@id/product_viewpager"
                    android:layout_width="fill_parent"
                    android:layout_height="290.0dip" />
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_marginLeft="4.0dip"
                android:layout_weight="1.0">
                <include
                    android:id="@id/layout_buy_pro"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10.0dip"
                    layout="@layout/layout_buy_pro" />
                <include
                    android:id="@id/recommend_buy_switch"
                    android:visibility="gone"
                    layout="@layout/layout_recommend_switch" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@id/btn_layout_buy"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/menu_item_height"
                    android:layout_marginTop="@dimen/activity_vertical_spacing">
                    <RelativeLayout
                        android:orientation="horizontal"
                        android:id="@id/btn_layout_enhancement"
                        android:background="@drawable/background_corner_trans_payed"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_marginRight="@dimen/activity_horizontal_spacing_big"
                        android:layout_weight="1.0">
                        <Button
                            android:textSize="@dimen/text_size_normal"
                            android:textColor="@color/color_product_payed"
                            android:gravity="center"
                            android:id="@id/btn_view_enhancement"
                            android:background="@null"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="4.0dip"
                            android:layout_centerInParent="true"
                            android:textAllCaps="false"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMinTextSize="@dimen/text_size_normal"
                            android:autoSizeMaxTextSize="@dimen/text_size_title" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:orientation="horizontal"
                        android:id="@id/btn_layout_pro"
                        android:background="@drawable/background_corner_trans_pro"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_weight="2.0">
                        <Button
                            android:textSize="@dimen/text_size_title"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@id/btn_pro"
                            android:background="@null"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="4.0dip"
                            android:layout_centerInParent="true"
                            android:lineSpacingMultiplier="0.7"
                            android:textAllCaps="false"
                            android:autoSizeTextType="uniform"
                            android:autoSizeMinTextSize="@dimen/text_size_normal"
                            android:autoSizeMaxTextSize="@dimen/text_size_title" />
                    </RelativeLayout>
                </LinearLayout>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <ProgressBar
        android:id="@id/ProgressBar"
        android:visibility="gone"
        android:layout_width="96.0dip"
        android:layout_height="96.0dip"
        android:layout_marginTop="32.0dip"
        android:layout_centerInParent="true"
        android:elevation="2.0dip"
        style="?android:progressBarStyle" />
    <TextView
        android:textSize="16.0sp"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@id/ProgressBarText"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_above="@id/ProgressBar"
        android:elevation="2.0dip" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>