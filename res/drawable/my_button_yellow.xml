<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#ffffe57f"
                android:endColor="#ffffab00"
                android:angle="270.0" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
    <item
        android:state_focused="true">
        <shape>
            <gradient
                android:startColor="#ffff6d00"
                android:endColor="#fffb9e1b"
                android:angle="270.0" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#fff9a825"
                android:endColor="#fffb9e1b"
                android:angle="270.0" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
</selector>