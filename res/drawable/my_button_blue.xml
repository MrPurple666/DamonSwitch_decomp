<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#ff82b1ff"
                android:endColor="#ff2962ff"
                android:angle="270.0" />
            <stroke
                android:width="0.0dip"
                android:color="#ffbdbdbd" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
    <item
        android:state_focused="true">
        <shape>
            <gradient
                android:startColor="#ffff6d00"
                android:endColor="#ffffd180"
                android:angle="270.0" />
            <stroke
                android:width="0.0dip"
                android:color="#ffbdbdbd" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ff0091e1"
                android:endColor="#ff80d8ff"
                android:angle="270.0" />
            <stroke
                android:width="0.0dip"
                android:color="#ffbdbdbd" />
            <corners
                android:radius="3.0dip" />
        </shape>
    </item>
</selector>